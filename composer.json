{
    "name": "antwerpes/barcodes-1d",
    "description": "Library for programmatically creating and rendering barcodes",
    "keywords": [
        "antwerpes",
        "barcodes",
        "ean13",
        "code128",
        "upc"
    ],
    "homepage": "https://github.com/antwerpes/barcodes",
    "license": "MIT",
    "authors": [
        {
            "name": "Elisha Witte",
            "email": "opensource@antwerpes.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-gd": "*",
        "illuminate/collections": "^9.13|^10.0|^11.0",
        "intervention/image": "^3.6",
        "meyfa/php-svg": "^0.15.0",
        "symfony/options-resolver": "^7.1"
    },
    "require-dev": {
        "chiiya/code-style-php": "^3.0",
        "phpro/grumphp": "^2.5",
        "phpunit/phpunit": "^11.2",
        "symfony/var-dumper": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Antwerpes\\Barcodes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Antwerpes\\Barcodes\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
